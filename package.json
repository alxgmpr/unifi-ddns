{
  "name": "unifi-ddns",
  "version": "2.0.0",
  "description": "A Cloudflare Worker script that provides a UniFi-compatible DDNS API to dynamically update the IP address of a DNS A record.",
  "main": "dist/index.js",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler publish",
    "format": "prettier --write '**/*.{js,ts,json}'",
    "typecheck": "npm run generate-types && tsc --noEmit",
    "generate-types": "wrangler types"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cloudflare": "^4.2.0"
  },
  "devDependencies": {
    "prettier": "3.5.3",
    "typescript": "^5.8.3",
    "wrangler": "^4.14.2"
  }
}
